<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ClearEditText">
        <attr name="leftImage" format="reference" />
        <attr name="rightImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="RectangleView">
        <attr name="angle" format="integer" />
        <attr name="backgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="angles" format="integer" />
    </declare-styleable>

    <declare-styleable name="StickerView">
        <attr name="showIcons" format="boolean" />
        <attr name="showBorder" format="boolean" />
        <attr name="bringToFrontCurrentSticker" format="boolean" />
        <attr name="borderColor" format="color" />
        <attr name="borderAlpha" format="integer" />
    </declare-styleable>



</resources>
